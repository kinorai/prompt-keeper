{
  "name": "prompt-keeper",
  "version": null,
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"next,worker\" -c \"cyan,magenta\" \"next dev --turbopack\" \"npm run dev:worker\"",
    "dev:next": "next dev --turbopack",
    "dev:worker": "tsx --env-file=.env src/workers/outbox.ts",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "generate-password": "node scripts/generate-password-hash.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "commitlint": "commitlint --edit",
    "worker:outbox": "node --env-file=.env ./dist/workers/outbox.js"
  },
