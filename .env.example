NODE_ENV="production"
LOG_LEVEL="info"

# LiteLLM Config
LITELLM_MASTER_KEY="sk-1234"
OPENAI_API_KEY="sk-1234"
ANTHROPIC_API_KEY="sk-1234"
DEEPSEEK_API_KEY="sk-1234"
GEMINI_API_KEY="sk-1234"
OPENROUTER_API_KEY="sk-1234"
LITELLM_URL="http://litellm:4000"

# Local Services Ports
OPENSEARCH_PORT=9200
OPENSEARCH_PERF_PORT=9600
LITELLM_PORT=4001
OPEN_WEBUI_PORT=8080

# OpenSearch
OPENSEARCH_URL="http://opensearch:9200"
OPENSEARCH_USERNAME="admin"
OPENSEARCH_PASSWORD="NotAGreatPassword2^"
OPENSEARCH_INDEX="prompt-keeper"

# Authentication (UI)
# Generate a secure random string for JWT signing
JWT_SECRET="your-jwt-secret-key-change-this-in-production"
ACCESS_TOKEN_SECRET="your-access-token-secret-key-change-this-in-production"
REFRESH_TOKEN_SECRET="your-refresh-token-secret-key-change-this-in-production"

# Username for UI login
AUTH_USERNAME="admin"
# APR1-MD5 hash generated with openssl passwd -apr1 "your_password_here" | sed 's/\$/\\$/g'
AUTH_PASSWORD_HASH="\$apr1\$vjgKU5UO\$SEZrMfpX0gQZjKJ23WZdV1"

# Authentication (Prompt Keeper API)
# API key for direct API access (e.g., /api/search)
PROMPT_KEEPER_API_KEY="sk-1234"

# CORS Configuration
# Comma-separated list of allowed origins (e.g., "example.com,another.com")
CORS_ORIGIN="*"
