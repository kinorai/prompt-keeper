NODE_ENV="production"
LOG_LEVEL="info"

# LiteLLM Config
LITELLM_MASTER_KEY="sk-1234"
OPENAI_API_KEY="sk-1234"
ANTHROPIC_API_KEY="sk-1234"
DEEPSEEK_API_KEY="sk-1234"
GEMINI_API_KEY="sk-1234"
OPENROUTER_API_KEY="sk-1234"
LITELLM_URL="http://litellm:4000"

# Local Services Ports
OPENSEARCH_PORT=9200
OPENSEARCH_PERF_PORT=9600
LITELLM_PORT=4001
OPEN_WEBUI_PORT=8080

# OpenSearch
OPENSEARCH_URL="http://opensearch:9200"
OPENSEARCH_USERNAME="admin"
OPENSEARCH_PASSWORD="NotAGreatPassword2^"
OPENSEARCH_INDEX="prompt-keeper"

# Postgres
POSTGRES_PRISMA_URL=postgresql://postgres:postgres@postgres:5432/prompt_keeper?schema=public
POSTGRES_URL_NON_POOLING=postgresql://postgres:postgres@postgres:5432/prompt_keeper?schema=public

# Worker polling interval (ms) and batch size
OUTBOX_POLL_INTERVAL_MS=1000
OUTBOX_BATCH_SIZE=50
# Optional worker shard id and total (for horizontal scaling)
WORKER_SHARD_ID=0
WORKER_SHARD_TOTAL=1

# Authentication (UI)
# Generate a secure random string for JWT signing
JWT_SECRET="your-jwt-secret-key-change-this-in-production"
ACCESS_TOKEN_SECRET="your-access-token-secret-key-change-this-in-production"
REFRESH_TOKEN_SECRET="your-refresh-token-secret-key-change-this-in-production"

NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-at-least-32-chars-long

# Username for UI login
AUTH_USERNAME="test@test.com"
# APR1-MD5 hash generated with openssl passwd -apr1 "test@test.comdad9:;;la,zdlkazA" | sed 's/\$/\\$/g'
AUTH_PASSWORD_HASH="\$apr1\$aVPtMi24\$XDG3I1TlI2Au54qg3boNs0"

# Authentication (Prompt Keeper API)
# API key for direct API access (e.g., /api/search)
PROMPT_KEEPER_API_KEY="sk-1234"

# CORS Configuration
# Comma-separated list of allowed origins (e.g., "example.com,another.com")
CORS_ORIGIN="*"

# S3 (Minio)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET_NAME=prompt-keeper

S3_REGION=us-east-1
S3_FORCE_PATH_STYLE=true
